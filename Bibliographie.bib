	journal = {Composites Science and Technology},
	keywords = {A. Carbon nanotubes,A. Polymer-matrix composites (PMCs),B. Interface,B. Surface treatments,D. Scanning electron microscopy (SEM)},
	mendeley-groups = {Doctorat/Carbon nanoparticles/CNTs/Surface free energy / dispersion},
	number = {14},
	pages = {1644--1651},
	publisher = {Elsevier Ltd},
	title = {{Correlation between electrokinetic potential, dispersibility, surface chemistry and energy of carbon nanotubes}},
