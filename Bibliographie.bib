	journal = {Composite Interfaces},
	keywords = {Thermoplastic composites,energy directors,mechanical properties,optical microscopy,plastic ultrasonic welding},
	mendeley-groups = {Engage DYNAMIC,Doctorat/Welding-Heating-Joining/Ultrasonic/Vibration},
	number = {2},
	pages = {203--214},
	publisher = {Taylor {\&} Francis},
	title = {{On the effect of flat energy directors thickness on heat generation during ultrasonic welding of thermoplastic composites}},
