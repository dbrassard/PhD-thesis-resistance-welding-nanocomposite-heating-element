	file = {:/triton.meca.polymtl.ca/usagers/dabrad/Documents/Mendeley Desktop/Yang, Pitchumani/Polymer Engineering {\&} Science/Yang, Pitchumani - 2002 - Interlaminar contact development during thermoplastic fusion bonding.pdf:pdf},
