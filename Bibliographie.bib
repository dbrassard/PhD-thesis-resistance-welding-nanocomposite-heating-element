	file = {:/triton.meca.polymtl.ca/usagers/dabrad/Documents/Mendeley Desktop/Schell et al/Journal of Materials Processing Technology/Schell et al. - 2009 - Computational and experimental analysis of fusion bonding in thermoplastic composites Influence of proce.pdf:pdf},
