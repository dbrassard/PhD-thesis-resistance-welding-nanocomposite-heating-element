	mendeley-groups = {Doctorat/Nanocomposites/Effect of nano on reptation time,Article{\_}modelisation{\_}soudage{\_}nanocomposite},
	number = {18},
	pages = {7091--7097},
	title = {{Polymer diffusion exhibits a minimum with increasing single-walled carbon nanotube concentration}},
	volume = {42},
	year = {2009}
}

