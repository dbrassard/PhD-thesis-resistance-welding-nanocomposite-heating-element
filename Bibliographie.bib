	journal = {Progress in Polymer Science},
	keywords = {Conductive polymer nanocomposites,Flexible nanocomposites,Graphene materials,Mechanical performance,Polymer interfaces},
	mendeley-groups = {Doctorat/Nanocomposites/Electrical properties,Doctorat/Nanocomposites},
	number = {11},
	pages = {1934--1972},
	publisher = {Elsevier Ltd},
	title = {{Graphene-polymer nanocomposites for structural and functional applications}},
	volume = {39},
