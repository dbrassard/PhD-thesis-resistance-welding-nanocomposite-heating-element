	file = {:/triton.meca.polymtl.ca/usagers/dabrad/Documents/Mendeley Desktop/Jiang, Drzal/Journal of Power Sources/Jiang, Drzal - 2012 - Exploring the potential of exfoliated graphene nanoplatelets as the conductive filler in polymeric nanocomposites.pdf:pdf},
