	isbn = {9783000533877},
	issn = {1359835X},
	journal = {Composites Part A: Applied Science and Manufacturing},
	keywords = {A. Polymer-matrix composites (PMCs),B. Adhesion,D. Microstructural analysis,E. Assembly},
	mendeley-groups = {Doctorat/Welding-Heating-Joining/Ultrasonic/Vibration},
	pages = {32--40},
	publisher = {Elsevier Ltd},
	title = {{Hybrid welding of carbon-fiber reinforced epoxy based composites}},
	url = {https://doi.org/10.1016/j.compositesa.2017.10.021},
