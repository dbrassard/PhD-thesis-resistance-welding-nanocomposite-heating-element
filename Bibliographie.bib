	issn = {00323861},
	journal = {Polymer},
	keywords = {Carbon nanotubes,Colorless polyimides,Nanocomposites},
	mendeley-groups = {Article{\_}heat{\_}dissipation{\_}nanocomposite,Doctorat/Nanocomposites/Applications,Doctorat/Carbon nanoparticles/CNTs/Applications},
	number = {3},
	pages = {825--836},
	title = {{Space durable polymer/carbon nanotube films for electrostatic charge mitigation}},
	volume = {45},
