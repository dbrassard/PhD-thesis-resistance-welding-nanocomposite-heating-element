	file = {:/triton.meca.polymtl.ca/usagers/dabrad/Documents/Mendeley Desktop/Pradhan et al/Nanotechnology/Pradhan et al. - 2009 - The specific heat and effective thermal conductivity of composites containing single-wall and multi-wall carbon.pdf:pdf},
