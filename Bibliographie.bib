	keywords = {A. Polymer-matrix composites (PMCs),Welding/joining},
	mendeley-groups = {Article{\_}modelisation{\_}soudage{\_}nanocomposite,Doctorat/Welding-Heating-Joining},
	number = {2001},
	pages = {839--857},
	title = {{Advances in fusion bonding techniques for joining thermoplastic matrix composites: A review}},
