	file = {:/triton.meca.polymtl.ca/usagers/dabrad/Documents/Mendeley Desktop/Thostenson, Chou/Journal of Physics D Applied Physics/Thostenson, Chou - 2002 - Aligned multi-walled carbon nanotube-reinforced composites processing and mechanical characterization.pdf:pdf},
