	isbn = {0014-3057},
	issn = {00143057},
	journal = {European Polymer Journal},
	keywords = {Composite,Electrical conductivity,Nanofiller,Tunnelling},
	mendeley-groups = {Doctorat/Electrical conductivity,Doctorat/Carbon nanoparticles/Graphene},
	number = {5},
	pages = {1066--1072},
