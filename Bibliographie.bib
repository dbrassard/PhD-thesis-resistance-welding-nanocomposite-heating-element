@article{Youngs2006a,
	abstract = {The dielectric relaxation due to interfacial charges in metal particle-filled composites occurs in the optical (high frequency) regime. However, many applications would benefit from a shift of the relaxation to a much lower frequency regime (e.g. radio or microwave range). This could be achieved by using a filler with reduced conductivity, but there is no continuum of conductivity in naturally occurring materials to allow engineers to readily achieve this aim or to have complete design freedom. Recently, it was demonstrated, experimentally and theoretically, how insulating particles with nano-scale metal coatings enable this relaxation to be shifted to a lower frequency regime. The current work investigates the microstructure of the metal coating, highlighting a semi-continuous coating structure. Therefore, the relevance of a two-dimensional (2D) percolation -based model to describe the effective dielectric properties of the coating is explored. It is demonstrated that a model in which the metal coating is assumed to be near the 2D percolation threshold can provide a reasonable fit to the experimental data. An improved fit is achieved when a distribution of metal area coverage is permitted.},
	author = {Youngs, I J and Bowler, N and Ugurlu, O},
	doi = {10.1088/0022-3727/39/7/S05},
	file = {:/triton.meca.polymtl.ca/usagers/dabrad/Documents/Mendeley Desktop/Youngs, Bowler, Ugurlu/Journal of Physics D Applied Physics/Youngs, Bowler, Ugurlu - 2006 - Dielectric relaxation in composites containing electrically isolated particles with thin se.pdf:pdf},
	isbn = {0022-3727},
	issn = {0022-3727},
	journal = {Journal of Physics D: Applied Physics},
	mendeley-groups = {Doctorat/Nanocomposites/Electrical properties},
	pages = {1312--1325},
	title = {{Dielectric relaxation in composites containing electrically isolated particles with thin semi-continuous metal coatings}},
	volume = {39},
	year = {2006}
}
