	issn = {00323861},
	journal = {Polymer},
	keywords = {Carbon nanotubes,Poly(ether ether ketone),Rheology},
	mendeley-groups = {Doctorat/Nanocomposites/PEEK CNT,Doctorat/Nanocomposites/Electrical properties/PEEK CNT,Doctorat/Nanocomposites/Electrical properties,Doctorat/Nanocomposites/Thermal properties,Article{\_}heat{\_}dissipation{\_}nanocomposite},
	number = {24},
