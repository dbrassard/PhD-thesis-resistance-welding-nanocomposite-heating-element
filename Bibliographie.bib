	keywords = {Bipolar plates,Electrical conductivity,Exfoliated graphene nanoplatelets,Mechanical properties,Nanocomposites},
	mendeley-groups = {Doctorat/Nanocomposites/Electrical properties},
	pages = {297--306},
	publisher = {Elsevier B.V},
	title = {{Exploring the potential of exfoliated graphene nanoplatelets as the conductive filler in polymeric nanocomposites for bipolar plates}},
