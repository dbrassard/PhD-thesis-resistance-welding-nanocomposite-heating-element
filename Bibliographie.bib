	isbn = {0959-9428},
	issn = {0959-9428},
	journal = {Journal of Materials Chemistry},
	mendeley-groups = {Doctorat/Nanocomposites/PEEK CNT,Doctorat/Nanocomposites/Electrical properties/PEEK CNT},
	number = {38},
	pages = {8247},
	title = {{Novel nanocomposites reinforced with hydroxylated poly(ether ether ketone)-grafted carbon nanotubes}},
