	keywords = {A. Polymer-matrix composites (PMCs),B. Debonding,B. Mechanical properties,E. Welding/joining,Resistance welding},
	mendeley-groups = {Doctorat/Welding-Heating-Joining/Resistance,Article{\_}soudage{\_}nano{\_}1},
