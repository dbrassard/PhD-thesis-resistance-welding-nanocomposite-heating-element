	isbn = {1998-0124},
	issn = {19980124},
	journal = {Nano Research},
	keywords = {Large-scale continuous synthesis,Numerical modeling,Optimization,Radio frequency (RF) induction thermal plasma,Single-walled carbon nanotubes (SWCNTs)},
	mendeley-groups = {Doctorat/Carbon nanoparticles/CNTs/Manufacturing},
	pages = {800--817},
	title = {{Synthesis of single-walled carbon nanotubes by induction thermal plasma}},
	volume = {2},
